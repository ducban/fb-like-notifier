{
  "name": "Facebook Like Notifier",
  "description": "",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
      "16": "img/icon_16.png",
      "48": "img/icon_48.png",
      "128": "img/icon_128.png"
  },
  "background": {
      "scripts": ["js/background.js"]
  },
  "minimum_chrome_version": "28.0",
  "permissions": [
    "webRequest",
    "notifications",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*"
  ]
}
